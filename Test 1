*****************************************************************************************************************************************************************
------------------------------------------------------------------------Palindrome number-----------------------------------------------------------------------
****************************************************************************************************************************************************************
import java.util.*;
public class Main
 {
	static boolean pal(int n)
	{
		int m = n;
		int r = 0;
		while(n>0)
		{
			r = r*10+(n%10);
			n = n/10;
		}
		return m==r;
	}
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(pal(n));

	}
}
*********************************************************************************************************************************************************************
------------------------------------------------------------Check AP-------------------------------------------------------------------------------------------------
*********************************************************************************************************************************************************************
import java.util.*;
public class Main 
{
	
	public static void main(String[] args)
	 {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int arr[] = new int[n];
		for (int i = 0; i < n; i++)
		{
			arr[i] = sc.nextInt();			
		}
		int d = arr[1] - arr[0];
		boolean ans = true;
		for (int i = 1; i < arr.length; i++)
		 {
			if (arr[i]-arr[i-1]!=d)
			{
				ans = false;
				break;
			}			
		}
		System.out.println(ans);

	}
}

********************************************************************************************************************************************************************
----------------------------------------------------------------------Number Star pattern 1-------------------------------------------------------------------------
*******************************************************************************************************************************************************************
import java.util.*;

public class runner 
{

	public static void main(String[] args)
	 {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 1; i <= n; i++)
		 {
			for (int j = n; j >= 1; j--)
			 {
				if (j == i)
				{
					System.out.print("*");
				} else {
					System.out.print(j);

				}
			}
			System.out.println();
		}

	}
}
